{
	"title": "Hai HHKB Launch app - chatfinity",
	"rules": [
		{
			"description": "right_option + C -> Chatfinity",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "c",
						"modifiers": {
							"mandatory": ["right_option"]
						}
					},
					"to": [
						{
							"shell_command": "open '/Applications/Chatfinity.app'"
						}
					]
				}
			]
		}
  ]
}
